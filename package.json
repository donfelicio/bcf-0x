    "author": "Leonid Logvinov <logvinov.leon@gmail.com>",
    "license": "Apache-2.0",
    "scripts": {
        "prettier": "yarn prettier:js && yarn prettier:ts",
        "prettier:js": "prettier --write 'js/**/*.js' --config .prettierrc",
        "start:js": "nodemon -r dotenv/config js/index.js",
        "download_snapshot": "cross-env-shell download --extract --strip 1 --out 0x_ganache_snapshot ${npm_package_config_snapshot_url}",
        "ganache-cli": "cross-env ganache-cli --noVMErrorsOnRPCResponse --gasLimit 10000000 --db 0x_ganache_snapshot  -p 8545 --networkId 50 -m \"luggage syrup unfair mixture inspire metal play fitness buzz ski betray found\""
    },
    "devDependencies": {
        "@0x/tslint-config": "^3.0.0",
        "@types/cors": "^2.8.4",
        "@types/dotenv": "^6.1.0",
        "@types/express": "^4.16.0",
        "@types/lodash": "^4.14.116",
        "@types/web3-provider-engine": "^14.0.0",
        "prettier": "^1.14.3",
        "shx": "^0.3.2",
        "tslint": "^5.11.0",
        "typescript": "^3.0.3"
    },
    "config": {
        "mnemonic": "concert load couple harbor equip island argue ramp clarify fence smart topic",
        "snapshot_url": "http://ganache-snapshots.0x.org.s3.amazonaws.com/0x_ganache_snapshot-2.4.0.zip"
    },
    "dependencies": {
        "0x.js": "^4.0.1",
        "@0x/assert": "^2.0.1",
        "@0x/connect": "^4.0.1",
        "@0x/contract-addresses": "^2.2.1",
        "@0x/json-schemas": "^3.0.1",
        "@0x/order-watcher": "^3.0.1",
        "@0x/subproviders": "^3.0.1",
        "@0x/types": "^2.0.1",
        "@0x/typescript-typings": "^4.0.0",
        "@0x/utils": "^4.0.2",
        "@0x/web3-wrapper": "^4.0.1",
        "@babel/polyfill": "^7.0.0",
        "body-parser": "^1.18.3",
        "cors": "^2.8.5",
        "cross-env": "^5.1.4",
        "dotenv": "^6.2.0",
        "download-cli": "^1.0.5",
        "express": "^4.16.3",
        "express-async-handler": "^1.1.4",
        "forever": "^0.15.3",
        "ganache-cli": "6.1.6",
        "http-status-codes": "^1.3.0",
